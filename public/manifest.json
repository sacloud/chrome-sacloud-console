{
  "name": "Sacloud Console",
  "manifest_version": 2,
  "version": "0.1.0",
  "description": "A Google Chrome extension to use CLI for SakuraCloud in the control panel",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": ["https://secure.sakura.ad.jp/cloud/iaas"],
  "content_scripts" : [
    {
      "matches": [ "https://secure.sakura.ad.jp/cloud/iaas*" ],
      "js": ["/static/js/content_script.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon_128.png",
    "default_popup": "index.html",
    "default_title": "Sacloud Console"
  }
}
